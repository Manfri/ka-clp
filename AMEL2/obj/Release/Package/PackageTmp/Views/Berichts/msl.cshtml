            <table class="table">
                
                    <tr>
                        <th>
                            AKZ
                        </th>
                        <th>
                            Bezeichnung
                        </th>
                        <th>
                            Örtlichkeit
                        </th>
                        <th>
                            ExZone
                        </th>
                        <th>
                            Fkt-PLT
                        </th>
                        <th>
                            Messprinzip
                        </th>
                        <th>
                            MB-Betr.
                        </th>
                        <th>
                            EH
                        </th>
                        <th>
                            VersorgungsSp.
                        </th>
                        <th>
                            Prozessanschluss
                        </th>
                        <th>
                            Werkstoff
                        </th>
                    </tr>
                
           
        
       
                    @foreach (var item in Model)
            {
                        <tr>
                            @if (item.IT == 0)
                {
                                <td class="table-cell-aliceblue">
                                    @Html.DisplayFor(modelItem => item.s1)
                                </td>
                                <td class="table-cell-aliceblue"></td>
                                <td class="table-cell-aliceblue"></td>
                                <td class="table-cell-aliceblue"></td>
                                <td class="table-cell-aliceblue"></td>
                                <td class="table-cell-aliceblue"></td>
                                <td class="table-cell-aliceblue"></td>
                                <td class="table-cell-aliceblue"></td>
                                <td class="table-cell-aliceblue"></td>
                                <td class="table-cell-aliceblue"></td>
                                <td class="table-cell-aliceblue"></td>
                            }
                            @if (item.IT != 0)
                {
                                <td>
                                    @Html.DisplayFor(modelItem => item.s1)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.s2)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.s3)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.s4)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.s5)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.s6)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.s7)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.s8)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.s9)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.s10)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.s11)
                                </td>
                            }
                            <!--
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { id=item.BerichtID }) |
                                @Html.ActionLink("Details", "Details", new { id=item.BerichtID }) |
                                @Html.ActionLink("Delete", "Delete", new { id=item.BerichtID })
                            </td>
                            -->
                        </tr>
                    }
                
